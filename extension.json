{
	"name": "BlueSpiceArticleInfo",
	"version": "3.0.0-alpha",
	"url": "https://help.bluespice.com/index.php/Articleinfo",
	"author": [
		"Markus Glaser",
		"Stephan Muggli"
	],
	"descriptionmsg": "bs-articleinfo-desc",
	"type": "bluespice",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"BlueSpiceFoundation": "*"
		}
	},
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceArticleInfo": {
					"className": "ArticleInfo",
					"extPath": "/BlueSpiceArticleInfo",
					"configDefinitions": {
						"ArticleInfoCheckRevisionInterval": "\\BlueSpice\\ArticleInfo\\ConfigDefinition\\CheckRevisionInterval::getInstance"
					}
				}
			}
		},
		"BlueSpiceArticleInfo": {
			"FlyoutModules": {}
		}
	},
	"MessagesDirs": {
		"BlueSpiceArticleInfo": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"ArticleInfoMagic": "languages/ArticleInfo.i18n.magic.php"
	},
	"AutoloadClasses": {
		"ArticleInfo": "ArticleInfo.class.php",
		"ViewStateBarTopElementCategoryShortList": "views/view.StateBarTopElementCategoryShortList.php"
	},
	"AutoloadNamespaces": {
		"BlueSpice\\ArticleInfo\\" : "src/"
	},
	"config_prefix": "bsg",
	"config": {
		"ArticleInfoCheckRevisionInterval": {
			"value": 10,
			"descriptionmsg": "bs-articleinfo-pref-CheckRevisionInterval",
			"public": true
		},
		"ArticleInfoImageLastEdited": {
			"value": "bs-infobar-last-edited.png"
		},
		"ArticleInfoImageLastEditor": {
			"value": "bs-infobar-author.png"
		},
		"ArticleInfoImageCategories": {
			"value": "bs-infobar-category.png"
		},
		"ArticleInfoImageSubpages": {
			"value": "bs-infobar-subpages.png"
		},
		"ArticleInfoImageCheckRevision": {
			"value": "bs-infobar-revision.png"
		}
	},
	"ResourceModules": {
		"ext.bluespice.articleinfo": {
			"scripts": "bluespice.articleInfo.js",
			"dependencies": [
				"ext.bluespice.extjs",
				"ext.bluespice"
			]
		},
		"ext.bluespice.articleinfo.flyout": {
			"scripts": "bluespice.articleInfo.flyout.js",
			"messages": [
				"bs-articleinfo-flyout-title",
				"bs-articleinfo-flyout-intro",
				"bs-articleinfo-nav-link-title"
			],
			"dependencies": [
				"ext.bluespice.articleinfo"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "BlueSpiceArticleInfo/resources"
	},
	"Hooks": {
		"SkinTemplateOutputPageBeforeExec": [
			"\\BlueSpice\\ArticleInfo\\Hook\\SkinTemplateOutputPageBeforeExec\\AddFlyout::callback"
		]
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
